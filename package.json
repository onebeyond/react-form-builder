{
  "name": "@guidesmiths/react-form-builder",
  "version": "3.6.0",
  "description": "React from builder using json schema",
  "author": "Guidesmiths",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidesmiths/react-form-builder.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "rollup -c -w",
    "build": "rollup -c",
    "prepare": "run-s build && husky install",
    "test": "npx @guidesmiths/license-checker --failOn AGPL --outputFileName License-report --errorReportFileName License-error | run-s test:unit test:lint test:build",
    "coverage": "npm run test:unit -- --coverage",
    "test:build": "run-s build",
    "test:lint": "eslint src --ext .js,.jsx --config .eslintrc --ignore-path .eslintignore --cache",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom  --passWithNoTests",
    "test:watch": "react-scripts test --env=jsdom",
    "eslint:fix": "eslint . --ext .js,.jsx --fix",
    "lint": "npm run test:lint",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "release:patch": "git tag -l | xargs git tag -d && changelog -p && git add CHANGELOG.md && git commit -m 'chore: updated CHANGELOG.md' && npm version patch && git push origin && git push origin --tags",
    "release:minor": "git tag -l | xargs git tag -d && changelog -m && git add CHANGELOG.md && git commit -m 'chore: updated CHANGELOG.md' && npm version minor && git push origin && git push origin --tags",
    "release:major": "git tag -l | xargs git tag -d && changelog -M && git add CHANGELOG.md && git commit -m 'chore: updated CHANGELOG.md' && npm version major && git push origin && git push origin --tags",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./public",
    "ncu": "npx ncu"
  },
  "peerDependencies": {
    "react": "^18.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@commitlint/cli": "^13.2.0",
    "@commitlint/config-conventional": "^13.2.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@sheerun/mutationobserver-shim": "^0.3.3",
    "@storybook/addon-actions": "^7.0.21",
    "@storybook/addon-essentials": "^7.0.21",
    "@storybook/addon-links": "^7.0.21",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^7.0.21",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "date-fns": "^2.16.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "generate-changelog": "^1.8.0",
    "gh-pages": "^2.2.0",
    "husky": "^8.0.3",
    "joi": "^17.4.1",
    "npm-check-updates": "^16.14.11",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "prettier": "^2.0.4",
    "react": "^18.0.6",
    "react-dom": "^18.0.6",
    "react-scripts": "^5.0.1",
    "react-select-event": "^5.3.0",
    "rollup": "^2.36.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-replace": "^2.2.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "debounce-promise": "^3.1.2",
    "install": "^0.13.0",
    "npm": "^8.16.0",
    "react-datepicker": "^4.13.0",
    "react-hook-form": "^7.44.3",
    "react-markdown": "^5.0.3",
    "react-phone-number-input": "^3.1.10",
    "react-select": "^5.7.3",
    "react-ymd-date-select": "^0.3.0",
    "theme-ui": "^0.15.7",
    "uuid": "^9.0.1"
  }
}
